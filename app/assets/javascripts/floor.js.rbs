class Floor < EcoModel
  def initialize(frame)
    super(frame,  "floor")
    self.cube( "floor_cube" , "floor")
    self.update()
  end

  def update()
    self.at("floor_cube").scale((@frame.width + 400)/2 , 10 , (@frame.length + 400)/2 )
    self.at("floor_cube").position(0, -@frame.height - @frame.size*2 , 0)
  end
end
